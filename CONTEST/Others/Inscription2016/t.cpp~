#include<bits/stdc++.h>
using namespace std;
#define T 10
#define N 10
#define M 10
int main(){
    freopen("t.txt","w",stdout);
    srand(time(NULL));
	int t = 1+rand()%T; cout<<t<<endl; 
	for(int i=1;i<=t;i++){
	    int n = 1+rand()%N,m = rand()%M;
	    cout<<n<<" "<<m<<endl;
	    for(int j=1;j<=m;j++){
    	    int u = 1+rand()%n,v = 1+rand()%n;
	        cout<<u<<" "<<v<<endl;    
	}
}
